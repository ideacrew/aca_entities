            <xsd:appinfo>
                <Base xmlns="http://niem.gov/niem/appinfo/2.0" i:name="Object"
                      i:namespace="http://niem.gov/niem/structures/2.0"/>
            </xsd:appinfo>
            <xsd:documentation source="">A data type for A restricted data type
                                         for social security number</xsd:documentation>
