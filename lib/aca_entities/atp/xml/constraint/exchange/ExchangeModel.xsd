    <xsd:import namespace="http://niem.gov/niem/structures/2.0"
        schemaLocation="../subset/niem/structures/2.0/structures.xsd"/>
    <xsd:element abstract="false" name="AccountTransferRequest" nillable="false"
        type="ext:AccountTransferRequestPayloadType"/>
