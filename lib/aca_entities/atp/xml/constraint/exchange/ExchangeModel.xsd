<?xml version="1.0" encoding="UTF-8"?>
<!--AT-041916-MD/XMLschemas/constraint/XMLschemas/exchange/ExchangeModel.xsd-->
<xsd:schema xmlns:exch="http://at.dsh.cms.gov/exchange/1.0"
