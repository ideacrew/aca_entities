      <xsd:simpleContent>
         <xsd:extension base="hix-types:CaretakerDependentCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
