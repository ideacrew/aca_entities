   <xsd:simpleType name="AbsentParentOrSpouseCodeSimpleType">
      <xsd:annotation>
         <xsd:appinfo>
            <i:Base i:name="Object" i:namespace="http://niem.gov/niem/structures/2.0"/>
         </xsd:appinfo>
         <xsd:documentation source="">A data type for an indication of whether or not the applicant has an absent parent or spouse.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="Yes">
            <xsd:annotation>
               <xsd:documentation source="">Yes, an insurance applicant has an absent parent or spouse, and information about those individuals is provided.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="No">
            <xsd:annotation>
               <xsd:documentation source="">No, an insurance applicant does not have an absent parent or spouse.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Yes-Exempt">
            <xsd:annotation>
               <xsd:documentation source="">Yes, an insurance applicant has an absent parent or spouse, but information about those individuals is not provided (which requires "good cause").</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType abstract="false" mixed="false" name="AbsentParentOrSpouseCodeType">
      <xsd:annotation>
         <xsd:appinfo>
            <i:Base i:name="Object" i:namespace="http://niem.gov/niem/structures/2.0"/>
         </xsd:appinfo>
         <xsd:documentation source="">A data type for an indication of whether or not the applicant has an absent parent or spouse.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="hix-types:AbsentParentOrSpouseCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:simpleType name="ActuarialValueMetallicTierCodeSimpleType">
